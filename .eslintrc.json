{
  "ignorePatterns": ["artifacts/", "cache/", "dist/", "types/", "contracts.ts"],
  "plugins": ["@typescript-eslint/eslint-plugin", "prettier"],
  "extends": [
    "plugin:@typescript-eslint/recommended",
